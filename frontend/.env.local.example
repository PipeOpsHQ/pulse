# Frontend Environment Variables
# Copy this file to .env.local and configure your values

# Sentry DSN for frontend error tracking
# Get this from your Pulse project settings
VITE_SENTRY_DSN=

# Environment name (development, staging, production)
VITE_SENTRY_ENVIRONMENT=development

# Trace sample rate (0.0 to 1.0)
# 1.0 = 100% of transactions, 0.1 = 10% of transactions
VITE_SENTRY_TRACES_SAMPLE_RATE=1.0
